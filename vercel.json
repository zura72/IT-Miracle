{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_MSAL_CLIENT_ID": "f536a53d-8a16-45cf-9acf-d8c77212b605",
    "REACT_APP_MSAL_AUTHORITY": "https://login.microsoftonline.com/94526da5-8783-4516-9eb7-8c58bbf66a2d",
    "REACT_APP_MSAL_REDIRECT_URI": "https://it.waskitainfrastruktur.co.id",
    "REACT_APP_API_BASE_URL": "https://it-backend-production.up.railway.app/api"
  },
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "build"
}